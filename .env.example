# API Configuration
# Base URL for the API - will use proxy in development, direct URL in production
VITE_API_BASE_URL=https://api.foodics.dev/v5

# API Authentication Token
# Replace with your actual Foodics API token
VITE_API_TOKEN=your_api_token_here

# Development proxy path (used when NODE_ENV=development)
VITE_DEV_API_PATH=/api

# Application Environment
NODE_ENV=development

# Optional: API Timeout (in milliseconds)
VITE_API_TIMEOUT=10000

# Optional: Enable API request logging
VITE_API_DEBUG=false
